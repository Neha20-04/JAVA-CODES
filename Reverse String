StringBuilder sb= new StringBuilder("ILOVE TO WATCH MOVIES");
    for(int i=0; i<sb.length()/2;i++){ // Wemake string to half and swap them
        int start=i;
        int end=sb.length()-1-i;
        
        char startchar= sb.charAt(start); // print start character
        char endchar=sb.charAt(end); //print end character
        
        sb.setCharAt(start,endchar); // replace start char with end char
        sb.setCharAt(end, startchar); // replace end char with start char
    }
