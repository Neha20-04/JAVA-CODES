import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        String name=sc.nextLine();
        // Here for string we use next line for ex: if we have 2 words in a name like Spider man and if we don't write Line we just write next() then only spider will be printed.
        //Check it out by yourself I'll include the Code
        // String name=sc.next();
        System.out.println("My name is"+" "+name);
        
        //CharAt  
        for(int i=0;i<name.length();i++){
            System.out.println(name.charAt(i)); 
        }
        //Compare 
        String name1=sc.nextLine();
        String name2= sc.nextLine();
        String myname=name1+name2;
        if(name1.compareTo(name2)==0){
            System.out.println("same");
        } else{
            System.out.println("Different");
        }
    }
}
